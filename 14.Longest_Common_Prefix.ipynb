   "source": [
    "class Solution:\n",
    "    def longestCommonPrefix(self, m):\n",
    "        if not m: return ''\n",
    "        s1 = min(m)\n",
    "        s2 = max(m)\n",
    "\n",
    "        for i, c in enumerate(s1):\n",
    "            if c != s2[i]:\n",
    "                return s1[:i] \n",
    "        return s1"
   ]
  }
